    last = 'wilson'
    domain = 'nsa'
    # return last five of last name in list splice
    f'{first[0]}{middle[0]}{last[0:5]}@{domain}.gov'

    # or
    '{}{}{}@{}.gov'.format(first[0],middle[0],last[0:5],domain)

    myEmal = 'nicholas.e.riesmeyer@usmc.mil
